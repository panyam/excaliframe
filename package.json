{
  "name": "excaliframe",
  "version": "0.0.5",
  "description": "Atlassian Forge app for Excalidraw drawings in Confluence",
  "scripts": {
    "update-version": "node scripts/update-version.js",
    "build": "npm run update-version && webpack --mode production",
    "dev": "webpack --mode development --watch",
    "type-check": "tsc --noEmit",
    "forge:deploy": "npm run build && forge deploy",
    "forge:install": "forge install",
    "forge:tunnel": "npm run build && forge tunnel"
  },
  "keywords": [
    "confluence",
    "atlassian",
    "forge",
    "excalidraw",
    "drawing",
    "whiteboard"
  ],
  "author": "Sriram Panyam",
  "license": "MIT",
  "dependencies": {
    "@excalidraw/excalidraw": "^0.18.0",
    "@forge/api": "^4.0.0",
    "@forge/bridge": "^4.0.0",
    "@forge/resolver": "^1.6.0",
    "mermaid": "^11.12.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@forge/cli": "^10.0.0",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^6.8.1",
    "eslint": "^8.57.0",
    "html-webpack-plugin": "^5.5.3",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
