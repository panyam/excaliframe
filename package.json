{
  "name": "excalfluence",
  "version": "1.0.0",
  "description": "Confluence plugin for custom drawing types starting with Excalidraw",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:webpack && npm run build:server",
    "build:webpack": "webpack --mode production",
    "build:server": "tsc -p tsconfig.server.json",
    "dev": "webpack --mode development --watch",
    "dev:server": "ts-node --transpile-only server.ts",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit",
    "update-url": "node scripts/update-baseurl.js"
  },
  "keywords": [
    "confluence",
    "plugin",
    "excalidraw",
    "drawing"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@excalidraw/excalidraw": "^0.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "express": "^4.18.2",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "html-webpack-plugin": "^5.5.3"
  }
}
