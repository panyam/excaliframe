{
  "name": "excaliframe",
  "version": "0.0.2",
  "description": "Confluence plugin for custom drawing types starting with Excalidraw",
  "scripts": {
    "build": "npm run update-version && npm run build:webpack && npm run copy-assets",
    "build:webpack": "webpack --mode production",
    "update-version": "node scripts/update-version.js",
    "copy-assets": "mkdir -p dist/images && cp atlassian-connect.json dist/ && cp -r images/* dist/images/ 2>/dev/null || true",
    "dev": "webpack --mode development --watch",
    "type-check": "tsc --noEmit",
    "update-url": "node scripts/update-baseurl.js"
  },
  "keywords": [
    "confluence",
    "plugin",
    "excalidraw",
    "drawing"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@excalidraw/excalidraw": "^0.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
