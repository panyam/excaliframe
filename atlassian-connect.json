{
  "key": "com.excaliframe.plugin",
  "name": "Excaliframe",
  "description": "Create and edit Excalidraw drawings directly in Confluence",
  "baseUrl": "https://excaliframe.appspot.com",
  "version": "1.0.0",
  "vendor": {
    "name": "Excaliframe",
    "url": "https://github.com/excaliframe"
  },
  "authentication": {
    "type": "jwt"
  },
  "lifecycle": {
    "installed": "/lifecycle/installed",
    "uninstalled": "/lifecycle/uninstalled"
  },
  "scopes": [
    "READ",
    "WRITE"
  ],
  "modules": {
    "dynamicContentMacros": [
      {
        "key": "excalidraw-macro",
        "bodyType": "plain-text",
        "name": {
          "value": "Excalidraw"
        },
        "description": {
          "value": "Embed an Excalidraw drawing"
        },
        "url": "/macro",
        "icon": {
          "url": "/images/excalidraw-icon.svg",
          "width": 24,
          "height": 24
        },
        "categories": ["visuals"],
        "outputType": "block",
        "editor": {
          "url": "/editor",
          "editTitle": {
            "value": "Edit Excalidraw Drawing"
          },
          "insertTitle": {
            "value": "Insert Excalidraw Drawing"
          },
          "width": "100%",
          "height": "100%"
        }
      }
    ]
  },
  "apiMigrations": {
    "gdpr": true,
    "context-qsh": true,
    "signed-install": true
  }
}
