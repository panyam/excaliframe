{
  "key": "com.excalfluence.plugin",
  "name": "Excalfluence",
  "description": "Custom drawing types for Confluence, starting with Excalidraw",
  "baseUrl": "http://localhost:3000",
  "version": "1.0.0",
  "authentication": {
    "type": "none"
  },
  "lifecycle": {
    "installed": "/lifecycle/installed",
    "uninstalled": "/lifecycle/uninstalled"
  },
  "scopes": [
    "read",
    "write"
  ],
  "modules": {
    "customContent": [
      {
        "key": "excalidraw-drawing",
        "name": {
          "value": "Excalidraw Drawing"
        },
        "description": {
          "value": "Create and edit Excalidraw drawings directly in Confluence"
        },
        "icon": {
          "url": "/images/excalidraw-icon.svg",
          "width": 24,
          "height": 24
        },
        "editor": {
          "url": "/editor.html"
        },
        "renderer": {
          "url": "/renderer.html"
        },
        "actions": [
          {
            "key": "edit",
            "label": {
              "value": "Edit"
            },
            "action": "edit"
          }
        ]
      }
    ]
  },
  "apiVersion": 1
}
